<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background" android:height="@dimen/seek_bar_custom_height" android:top="@dimen/seek_bar_custom_top_margin">
        <shape android:shape="rectangle">
            <size android:height="@dimen/seek_bar_custom_height"/>
            <solid android:color="@color/seek_bar_background"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress" android:height="@dimen/seek_bar_custom_height" android:top="@dimen/seek_bar_custom_top_margin">
        <clip>
            <shape>
                <size android:height="@dimen/seek_bar_custom_height"/>
                <solid android:color="@color/seek_bar_progress"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress" android:height="@dimen/seek_bar_custom_height" android:top="@dimen/seek_bar_custom_top_margin">
        <clip>
            <shape>
                <size android:height="@dimen/seek_bar_custom_height"/>
                <solid android:color="@color/seek_bar_secondary_progress"/>
            </shape>
        </clip>
    </item>
</layer-list>
