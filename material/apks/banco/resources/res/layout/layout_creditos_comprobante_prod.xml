<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent">
    <ScrollView android:id="@+id/viewComprobanteCreditos" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_above="@+id/creditos.ircuentas">
        <LinearLayout android:orientation="vertical" android:background="@color/white" android:layout_width="match_parent" android:layout_height="wrap_content">
            <include android:id="@+id/title_page4" layout="@layout/title_simple_layout"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.importe" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID156_CREDITS_VIEW_LBL_IMPORTQUERY" app:selectable="false" app:styleColumn="style_1"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.importeNeto" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID157_CREDITS_VIEW_LBL_RAWIMPORT" app:selectable="false" app:styleColumn="style_1"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.cuentaDestino" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID158_CREDITS_VIEW_LBL_TOACCOUNT" app:selectable="false" app:styleColumn="style_1"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.cantidadCuotas" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID159_CREDITS_VIEW_LBL_NUMCUOTES" app:selectable="false" app:styleColumn="style_1"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.tipoTasa" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID160_CREDITS_VIEW_LBL_TYPERATE" app:selectable="false" app:styleColumn="style_1"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.tasaEfectAnual" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID160_CREDITS_VIEW_LBL_EFECT_ANUAL" app:selectable="false" app:styleColumn="style_1"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.tasaCFTNA" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID160_CREDITS_VIEW_LBL_CFTNA" app:selectable="false" app:styleColumn="style_4"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.tasaCFTNAIVA" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID160_CREDITS_VIEW_LBL_CFTNAIVA" app:selectable="false" app:styleColumn="style_4"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.tasaNominal" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID161_CREDITS_VIEW_LBL_YEARLYRATE" app:selectable="false" app:styleColumn="style_1"/>
            <View style="@style/SeparatorSectionTitle"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID162_CREDITS_VIEW_LBL_FIRSTCUOTE" app:selectable="false" app:styleColumn="style_2" app:value=""/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.fechaPrimerVencimiento" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID163_CREDITS_VIEW_LBL_FIRSTEXPIRATION" app:selectable="false" app:styleColumn="style_1"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.capitalIntereses" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID164_CREDITS_VIEW_LBL_CAPITAL" app:selectable="false" app:styleColumn="style_1"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.cargoSeguro" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID165_CREDITS_VIEW_LBL_CHARGE" app:selectable="false" app:styleColumn="style_1"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.iva" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID166_CREDITS_VIEW_LBL_IVA" app:selectable="false" app:styleColumn="style_1"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.otrosImpuestos" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="Otros impuestos" app:selectable="false" app:styleColumn="style_1"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.importeCuota" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID167_CREDITS_VIEW_LBL_IMPORT" app:selectable="false" app:styleColumn="style_1"/>
            <ar.com.santander.rio.mbanking.view.tables.RowTwoColumnViewStyled android:id="@+id/credito.page4.numeroComprobante" android:layout_width="match_parent" android:layout_height="wrap_content" app:label="@string/ID168_CREDITS_VIEW_LBL_RECEIPTNUMBER" app:selectable="false" app:styleColumn="style_1"/>
            <TextView android:id="@+id/credito.page4.leyenda" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="@dimen/general_margin" style="@style/generic_leyend_light"/>
            <TextView android:id="@+id/credito.page4.leyenda2" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/general_margin" android:layout_marginRight="@dimen/general_margin" android:layout_marginBottom="@dimen/general_margin" android:text="@string/ID169_CREDITS_VIEW_LBL_SEUO" style="@style/generic_leyend_dark"/>
            <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
                <include layout="@layout/layout_bottom_receipt_share"/>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
    <Button android:id="@+id/creditos.ircuentas" android:text="@string/ID170_CREDITS_VIEW_BTN_GOTOACCOUNT" android:layout_alignParentBottom="true" style="@style/generic_boton_rojo"/>
</RelativeLayout>
