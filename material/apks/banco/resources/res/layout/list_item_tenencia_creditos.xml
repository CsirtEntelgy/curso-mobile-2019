<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/F10_00_rll_tipo" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="18dp" android:layout_marginTop="18dp" android:layout_toLeftOf="@+id/F10_00_img_adapter_fecha">
        <TextView android:textSize="15dp" android:textStyle="bold" android:textColor="@color/grey_medium_dark" android:id="@+id/F10_00_lbl_adapter_nro_prestamo" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="" android:layout_alignParentLeft="true" android:layout_centerVertical="true" app:fontPath="fonts/OpenSans-Bold.otf"/>
    </RelativeLayout>
    <ImageView android:id="@+id/F10_00_img_adapter_fecha" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="16dp" android:layout_marginRight="15dp" android:src="@drawable/ic_arrow_small" android:layout_alignParentRight="true" android:contentDescription=""/>
    <RelativeLayout android:id="@+id/F10_00_rll_tasa" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="18dp" android:layout_below="@+id/F10_00_rll_tipo" android:layout_alignLeft="@+id/F10_00_rll_tipo" android:layout_alignRight="@+id/F10_00_rll_tipo">
        <TextView android:textSize="15dp" android:textColor="@color/grey_medium_dark" android:id="@+id/F10_00_lbl_adapter_nro_cuota" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Cuota" android:layout_alignParentLeft="true" app:fontPath="fonts/OpenSans-Regular.otf"/>
        <TextView android:textSize="15dp" android:textColor="@color/grey_medium_dark" android:id="@+id/F10_00_lbl_adapter_capital_cuota_data" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:text="" android:layout_toRightOf="@+id/F10_00_lbl_adapter_nro_cuota" app:fontPath="fonts/OpenSans-Regular.otf"/>
    </RelativeLayout>
    <RelativeLayout android:id="@+id/F10_00_rll_capital" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/F10_00_rll_tasa" android:layout_alignLeft="@+id/F10_00_rll_tipo" android:layout_alignRight="@+id/F10_00_rll_tipo">
        <TextView android:textSize="15dp" android:textColor="@color/grey_medium_dark" android:id="@+id/F10_00_lbl_adapter_vencimiento" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Vencimiento" android:layout_alignParentLeft="true" app:fontPath="fonts/OpenSans-Regular.otf"/>
    </RelativeLayout>
    <View android:id="@+id/F10_00_separadorItem" android:background="@color/grey_medium_light" android:layout_width="match_parent" android:layout_height="@dimen/height_line_separator" android:layout_marginTop="24dp" android:layout_below="@+id/F10_00_rll_capital"/>
</RelativeLayout>
