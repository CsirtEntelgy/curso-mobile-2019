    <View android:id="@+id/F26_04_GROUP_SEPARATOR_BOTTOM" android:visibility="gone" android:layout_below="@+id/F26_04_RLT_HEADER_DATA_WRAPPER" style="@style/StrongSeparatorSectionGrey"/>
</RelativeLayout>
