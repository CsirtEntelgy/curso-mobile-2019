<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:background="@color/container_area_light_gray" android:layout_width="match_parent" android:layout_height="wrap_content">
    <View android:id="@+id/F26_04_GROUP_SEPARATOR_TOP" android:visibility="gone" android:layout_alignParentTop="true" style="@style/StrongSeparatorSectionGrey"/>
    <RelativeLayout android:id="@+id/F26_04_RLT_HEADER_DATA_WRAPPER" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="18dp" android:layout_marginTop="14dp" android:layout_marginRight="18dp" android:layout_marginBottom="10dp">
        <ImageView android:gravity="start" android:id="@+id/F26_04_BTN_EXPAND" android:background="@drawable/arrow_down" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="8dp" android:layout_centerVertical="true"/>
        <TextView android:textSize="13dp" android:textColor="@color/grey_medium" android:gravity="center_vertical" android:id="@+id/F26_04_LBL_DATA_TITULAR" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_toRightOf="@+id/F26_04_BTN_EXPAND" android:layout_centerVertical="true" fontPath="fonts/OpenSans-Bold.otf"/>
