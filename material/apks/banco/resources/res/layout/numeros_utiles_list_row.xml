        <TextView android:id="@+id/numeros_utiles_titulo" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/general_margin" android:layout_marginTop="@dimen/general_margin" android:layout_marginRight="@dimen/general_margin" style="@style/generic_title"/>
        <TextView android:id="@+id/numeros_utiles_detalle" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/general_margin" android:layout_marginRight="@dimen/general_margin" android:layout_marginBottom="@dimen/general_margin" style="@style/generic_bottomDescription_amount"/>
    </LinearLayout>
    <ar.com.santander.rio.mbanking.view.TelephoneRow android:id="@+id/numeros_utiles_telephoneRow1" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="1dp" android:src="@drawable/ic_call" android:contentDescription="@string/ACCESSIBILITY_BOTON_UBICACION"/>
    <ar.com.santander.rio.mbanking.view.TelephoneRow android:id="@+id/numeros_utiles_telephoneRow2" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="1dp" android:src="@drawable/ic_call"/>
</LinearLayout>
