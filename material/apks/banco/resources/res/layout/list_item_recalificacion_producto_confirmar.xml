<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:background="@color/white" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="vertical" android:paddingLeft="18dp" android:paddingTop="18dp" android:paddingRight="18dp" android:paddingBottom="16dp" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:id="@+id/tvRecalificacionLimiteProductoNombre" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="18dp" style="@style/generic_list_data_column2"/>
        <LinearLayout android:orientation="horizontal" android:id="@+id/llLimiteActual" android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:gravity="left" android:id="@+id/tvRecalificacionLimiteActualLabel" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/ID_4856_RECALIFICACION_LBL_LIMITE_ACTUAL" android:layout_weight="0.5" style="@style/generic_list_data_column1"/>
            <TextView android:gravity="right" android:id="@+id/tvRecalificacionLimiteActualValue" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="0.5" style="@style/generic_list_data_column2"/>
        </LinearLayout>
        <LinearLayout android:orientation="horizontal" android:id="@+id/llNuevoLimite" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="2dp">
            <TextView android:textColor="@color/black" android:gravity="left" android:id="@+id/tvRecalificacionNuevoLimiteLabel" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/ID_4850_RECALIFICACION_LBL_NUEVO_LIMITE" android:layout_weight="0.5" style="@style/generic_list_data_column1"/>
            <TextView android:gravity="right" android:id="@+id/tvRecalificacionNuevoLimiteValue" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="0.5" style="@style/generic_list_data_column2"/>
        </LinearLayout>
        <LinearLayout android:orientation="horizontal" android:id="@+id/llNuevoDisponible" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="2dp">
            <TextView android:textColor="@color/black" android:gravity="left" android:id="@+id/tvRecalificacionNuevoDisponibleLabel" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/ID_4851_RECALIFICACION_LBL_NUEVO_DISPONIBLE" android:layout_weight="0.5" style="@style/generic_list_data_column1"/>
            <TextView android:gravity="right" android:id="@+id/tvRecalificacionNuevoDisponibleValue" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="0.5" style="@style/generic_list_data_column2"/>
        </LinearLayout>
        <TextView android:gravity="center" android:id="@+id/tvRecalificacionMessageValue" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="10dp" style="@style/generic_list_data_column2"/>
    </LinearLayout>
    <View android:id="@+id/F27_00_ITEM_SEPARATOR_BOTTOM" style="@style/SeparatorListView"/>
</LinearLayout>
