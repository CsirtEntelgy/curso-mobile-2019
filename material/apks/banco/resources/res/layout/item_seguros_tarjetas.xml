<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:gravity="center_vertical" android:orientation="vertical" android:background="@color/white" android:layout_width="match_parent" android:layout_height="80dp">
    <RelativeLayout android:gravity="center" android:id="@+id/F27_11_RLAYOUT_ITEM_SEGUROS_TARJ" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="18dp" android:layout_marginTop="22dp" android:layout_marginRight="18dp" android:layout_alignParentTop="true">
        <CheckBox android:gravity="end" android:id="@+id/F27_11_BTN_CHECK" android:background="@drawable/check" android:clickable="false" android:layout_width="24dp" android:layout_height="24dp" android:button="@null" android:layout_alignParentRight="true"/>
        <LinearLayout android:orientation="horizontal" android:id="@+id/F27_11_ROW_TARJ" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginRight="8dp" android:layout_toLeftOf="@+id/F27_11_BTN_CHECK">
            <TextView android:id="@+id/F27_11_LBL_DATA_NUM_TARJ" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_toRightOf="@+id/F27_11_LBL_DATA_TIPO_TARJ" style="@style/generic_row_data_form"/>
