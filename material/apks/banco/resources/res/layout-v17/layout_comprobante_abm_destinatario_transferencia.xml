<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent">
    <ScrollView android:id="@+id/F23_22_scrollView" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_above="@+id/F23_22_LNL_BOTTOM_OBJECTS" android:layout_alignParentTop="true">
        <LinearLayout android:orientation="vertical" android:id="@+id/F23_22_LNL_TOP_OBJECTS" android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:id="@+id/F23_22_LBL_TITLE" android:layout_marginLeft="18dp" android:layout_marginTop="18dp" android:layout_marginRight="18dp" android:layout_marginBottom="12dp" android:text="@string/IDXX_TRANSFERENCE_EDIT_ADDRESSEE_RECEIPT_TITLE" android:layout_marginStart="18dp" android:layout_marginEnd="18dp" style="@style/generic_title"/>
            <View style="@style/SeparatorSectionTitle"/>
            <TableLayout android:orientation="vertical" android:id="@+id/F23_22_TABLE" android:layout_width="match_parent" android:layout_height="wrap_content"/>
            <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="18dp" android:lineSpacingExtra="3dp">
                <include layout="@layout/layout_bottom_receipt_share"/>
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>
    <LinearLayout android:orientation="vertical" android:id="@+id/F23_22_LNL_BOTTOM_OBJECTS" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_alignParentBottom="true">
        <Button android:id="@+id/F23_22_BTN_TRANSFERIR" android:text="@string/IDXX_TRANSFERENCE_ABM_ADDRESSEE_BTN_TRANSFER" android:textAllCaps="false" style="@style/generic_boton_rojo"/>
        <Button android:id="@+id/F23_22_BTN_AGENDA" android:text="@string/ID_3803_TRANSFERENCE_AGENDA_DESRINATARIOS" android:textAllCaps="false" style="@style/generic_boton_blanco_border"/>
    </LinearLayout>
</RelativeLayout>
