<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:background="@color/white" android:layout_width="match_parent" android:layout_height="wrap_content">
    <RelativeLayout android:id="@+id/F26_00_rll_main" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="18dp">
        <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/F26_00_lnl_globito" android:layout_width="70dp" android:layout_height="70dp" android:layout_alignParentLeft="true" android:layout_centerVertical="true" android:layout_alignParentStart="true">
            <TextView android:textSize="30sp" android:textColor="@color/generic_black" android:id="@+id/F26_LISTA_VIAJE_FRAGMENT_DATA_FECHA_DIA" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="-10dp" android:text="23" style="@style/generic_fontLight"/>
            <TextView android:textSize="15sp" android:textColor="@color/generic_black" android:id="@+id/F26_LISTA_VIAJE_FRAGMENT_DATA_FECHA_MES" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Marzo" android:importantForAccessibility="no" style="@style/generic_fontSemibold"/>
        </LinearLayout>
        <LinearLayout android:orientation="vertical" android:id="@+id/F26_00_lnl_datos" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="28dp" android:layout_toLeftOf="@+id/F26_00_rll_img_nav" android:layout_toRightOf="@+id/F26_00_lnl_globito" android:layout_centerVertical="true" android:layout_marginStart="28dp" android:layout_toStartOf="@+id/F26_00_rll_img_nav" android:layout_toEndOf="@+id/F26_00_lnl_globito">
            <LinearLayout android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <TextView android:textSize="15sp" android:textColor="@color/grey_medium_dark" android:id="@+id/F26_LISTA_VIAJE_FRAGMENT_DATA_CANTIDAD_DIAS" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="10dp" android:maxLines="1" android:layout_marginEnd="10dp" app:fontPath="fonts/OpenSans-Bold.otf"/>
                <TextView android:id="@+id/F26_LISTA_VIAJE_FRAGMENT_LBL_DIAS" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/F26_00_ITEM_DIAS" android:maxLines="1" style="@style/generic_label_marcacion"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <TextView android:textSize="15sp" android:textColor="@color/grey_medium_dark" android:id="@+id/F26_LISTA_VIAJE_FRAGMENT_DATA_CANTIDAD_DESTINOS" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="10dp" android:maxLines="1" android:layout_marginEnd="10dp" app:fontPath="fonts/OpenSans-Bold.otf"/>
                <TextView android:id="@+id/F26_LISTA_VIAJE_FRAGMENT_LBL_DESTINOS" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/F26_00_ITEM_DESTINOS" android:maxLines="1" style="@style/generic_label_marcacion"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <TextView android:textSize="15sp" android:textColor="@color/grey_medium_dark" android:id="@+id/F26_LISTA_VIAJE_FRAGMENT_DATA_CANTIDAD_TARJETAS" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="10dp" android:maxLines="1" android:layout_marginEnd="10dp" app:fontPath="fonts/OpenSans-Bold.otf"/>
                <TextView android:id="@+id/F26_LISTA_VIAJE_FRAGMENT_LBL_TARJETAS" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/F26_00_ITEM_TARJETAS" android:maxLines="1" style="@style/generic_label_marcacion"/>
            </LinearLayout>
        </LinearLayout>
        <RelativeLayout android:id="@+id/F26_00_rll_img_nav" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentRight="true" android:layout_centerVertical="true" android:layout_alignParentEnd="true">
            <ImageView android:id="@+id/F26_lista_viaje_fragment_arrow" android:layout_width="24dp" android:layout_height="24dp" android:src="@drawable/big_red_arrow" android:importantForAccessibility="no"/>
        </RelativeLayout>
    </RelativeLayout>
    <View android:id="@+id/F26_00_item_separator" style="@style/SeparatorListView"/>
</LinearLayout>
