            <TextView android:textSize="14dp" android:textColor="@color/generic_grey_dark" android:id="@+id/F27_31_LBL_DATA_DESCRIPCION2" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="5dp" fontPath="fonts/OpenSans-Light.otf"/>
        </LinearLayout>
    </RelativeLayout>
    <View android:id="@+id/F27_31_ITEM_SEPARATOR_BOTTOM" android:visibility="visible" android:layout_marginTop="10dp" android:layout_alignParentBottom="true" style="@style/SeparatorListView"/>
